{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "!/dist/demo",
    "!/dist/test"
  ],
  "keywords": [
    "API",
    "Bitcoin",
    "Coinbase",
    "crypto",
    "cryptocurrency",
    "currency",
    "Ethereum",
    "finance",
    "fintech",
    "order",
    "payments",
    "trading",
    "TypeScript",
    "withdraw"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">= 10.9",
    "yarn": ">= 1"
  },
  "name": "coinbase-pro-universal",
  "author": "Yadir Hernandez Batista",
  "module": "dist/coinbase-pro-universal.esm.js",
  "homepage": "https://benny.work/coinbase-pro-node",
  "description": "Universal Coinbase Pro API runs in Node.js, Expo (React-Native), Web, written in TypeScript and covered by tests.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadirhb/coinbase-pro-universal.git"
  },
  "bugs": {
    "url": "https://github.com/yadirhb/coinbase-pro-universal/issues"
  },
  "dependencies": {
    "axios": "0.19.2",
    "axios-retry": "3.1.8",
    "big.js": "5.2.2",
    "crypto-ts": "https://github.com/yadirhb/crypto-ts",
    "events": "3.1.0",
    "query-string": "6.13.1",
    "reconnecting-websocket": "4.4.0",
    "util": "0.12.3"
  },
  "devDependencies": {
    "@types/big.js": "4.0.5",
    "@types/jest": "^26.0.4",
    "@types/ws": "7.2.6",
    "husky": "^4.2.5",
    "nock": "13.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typedoc": "0.17.8",
    "typedoc-plugin-markdown": "2.3.1",
    "typescript": "^3.9.6"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  }
}
